{
    "name": "av/bonefish-utility",
    "description": "Bonefish utility classes",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": ">=5.6.0",
      "av/bonefish-traits": "^1.0",
      "doctrine/cache": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Bonefish\\Utility\\": ["src"],
            "Bonefish\\Tests\\Utility\\": ["tests"]
        }
